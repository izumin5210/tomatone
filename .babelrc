{
  "presets": [
    "es2015",
    "stage-2",
    "react",
    "power-assert"
  ],
  "plugins": [
    "syntax-flow",
    "transform-flow-strip-types",
    "transform-class-properties",
    "transform-decorators-legacy",
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "presets": [
        "react-optimize"
      ]
    }
  }
}
