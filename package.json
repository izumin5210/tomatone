{
  "name": "tomatone",
  "description": "Pluggable pomodoro timer",
  "version": "0.3.1",
  "author": {
    "name": "Masayuki IZUMI",
    "email": "masayuki@izumin.info",
    "url": "https://github.com/izumin5210"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "bugs": {
    "url": "https://github.com/izumin5210/tomatone/issues"
  },
  "build": {
    "appId": "info.izumin.tomatone",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "src/icon*.png",
      "dist/",
      "package.json",
      "resources/"
    ]
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.16.0",
    "dexie": "^1.5.1",
    "font-awesome": "^4.7.0",
    "fuse.js": "^2.6.1",
    "immutable": "^3.8.1",
    "menubar": "^5.1.0",
    "moment": "^2.16.0",
    "promised-reducer": "^0.1.0",
    "react": "^15.4.0",
    "react-addons-css-transition-group": "^15.4.1",
    "react-autocomplete": "izumin5210/react-autocomplete.git#build-pull201",
    "react-dispatcher-decorator": "^0.1.5",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.4.0",
    "react-router": "^4.0.0-alpha.6",
    "sanitize.css": "^4.1.0",
    "source-map-support": "^0.4.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-unassert": "^2.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^6.18.0",
    "conventional-changelog-cli": "^1.2.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.25.0",
    "devtron": "^1.4.0",
    "electron": "^1.4.8",
    "electron-builder": "^8.6.0",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-inspector": "^0.1.3",
    "electron-packager": "^8.2.0",
    "electron-rebuild": "^1.4.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-jsx": "^3.2.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "fake-indexeddb": "^1.0.8",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.37.4",
    "flowtype-loader": "^0.2.2",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^3.1.2",
    "npm-run-all": "^4.0.0",
    "postcss-apply": "^0.4.0",
    "postcss-color-function": "^2.0.1",
    "postcss-csso": "^1.1.2",
    "postcss-custom-properties": "^5.0.1",
    "postcss-flexbugs-fixes": "^2.0.0",
    "postcss-loader": "^1.1.1",
    "postcss-nesting": "^2.3.1",
    "postcss-reporter": "^2.0.0",
    "postcss-smart-import": "^0.6.6",
    "power-assert": "^1.4.2",
    "promise-test-helper": "^0.2.1",
    "react-addons-test-utils": "^15.4.0",
    "react-dnd-test-backend": "^1.0.2",
    "react-hot-loader": "next",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "stylelint-config-10up": "github:jonathantneal/stylelint-config-10up",
    "stylelint-config-standard": "^14.0.0",
    "stylelint-declaration-use-variable": "^1.6.0",
    "stylelint-selector-bem-pattern": "^1.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0-rc.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-merge": "^0.15.0"
  },
  "directories": {
    "buildResources": "resources",
    "output": "release"
  },
  "homepage": "https://github.com/izumin5210/tomatone#readme",
  "license": {
    "type": "MIT",
    "url": "http://izumin.mit-license.org/2015"
  },
  "main": "dist/main.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/izumin5210/tomatone.git"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:main": "NODE_ENV=production NODE_TARGET=main webpack --profile --colors",
    "build:renderer": "NODE_ENV=production NODE_TARGET=renderer webpack --profile --colors",
    "clean": "rm -rf dist release",
    "dev": "npm run server -- --start-dev",
    "generate:changelog": "conventional-changelog --preset angular --infile CHANGELOG.md --same--file",
    "generate:migration": "touch ./src/db/schemata/$(date +'%Y%m%d%H%M%S').js",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "npm-run-all --parallel lint:js:*",
    "lint:js:src": "eslint src",
    "lint:js:test": "eslint --global assert --global React test",
    "package": "build",
    "package:release": "npm run package -- --publish onTag --draft=false",
    "prebuild": "npm run clean",
    "prepackage": "npm run build",
    "pretest": "npm-run-all --parallel typecheck lint",
    "preversion": "npm test",
    "server": "NODE_ENV=development node --max_old_space_size=2096 -r babel-register ./scripts/server.js",
    "start": "HOT=1 NODE_ENV=development electron --debug=5858 -r babel-register -r babel-polyfill ./src/main.js",
    "start:production": "electron .",
    "test": "npm-run-all --parallel test:*",
    "test:js": "NODE_PATH=$PWD/src mocha",
    "typecheck": "flow",
    "version": "npm run generate:changelog && git add CHANGELOG.md"
  }
}
