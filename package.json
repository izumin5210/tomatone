  "scripts": {
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "NODE_ENV=production NODE_TARGET=main webpack --progress --profile --colors",
    "build:renderer": "NODE_ENV=production NODE_TARGET=renderer webpack --progress --profile --colors",
    "dev": "npm run server -- --start-dev",
    "server": "NODE_ENV=development node --max_old_space_size=2096 -r babel-register ./scripts/server.js",
    "start": "HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./src/main.js",
    "start:production": "electron .",
